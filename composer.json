{
    "name": "softdreams/laravel-vuex-crud",
    "description": "A sweet crud generator for laravel with vue/vuex frontend",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "illuminate/routing": "5.6.*",
        "illuminate/support" : "5.6.*",
        "illuminate/console": "5.6.*",
        "illuminate/config": "5.6.*",
        "illuminate/filesystem": "5.6.*"
    },
    "license": "mit",
    "authors": [
        {
            "name": "Ionut Dumitru",
            "email": "ionut.dumitru@webland.ro"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "SoftDreams\\LaravelVuexCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoftDreams\\LaravelVuexCrud\\LaravelVuexCrudProvider"
            ]
        }
    }}
